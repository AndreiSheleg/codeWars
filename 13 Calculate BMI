function bmi(weight, height) {
  let result = "";
  let bmi = weight / (height * height);
  if (bmi <= 18.5) {
    result = "Underweight";
  } else if (bmi <= 25) {
    result = "Normal";
  } else if (bmi <= 30) {
    result = "Overweight";
  } else if (bmi > 30) {
    result = "Obese";
    };
  return result;
}
